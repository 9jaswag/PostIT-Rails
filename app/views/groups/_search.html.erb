<% if !users.empty? %>
  <% users.each do |user| %>
    <div class="col-sm-2 col-2">
      <%= link_to "@#{user.username}", :back, class: "badge badge-primary user-chip" %>
    </div>
  <% end %>
<% else %>
  <div class="col-sm">
  <span class="badge badge-primary user-chip">User not found</span>
  </div>
<% end %>
